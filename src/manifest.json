{
  "manifest_version": 3,
  "name": "Chat with Google AI",
  "version": "1.0",
  "permissions": ["background", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://generativelanguage.googleapis.com/"
  ]
}
