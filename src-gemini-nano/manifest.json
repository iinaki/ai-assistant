{
    "manifest_version": 3,
    "name": "My AI Extension",
    "version": "1.0",
    "description": "A simple AI-powered Chrome extension",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
    }
}
